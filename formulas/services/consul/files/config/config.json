{
    "server": true,
    "datacenter": "dc1",
    "node_name": "consul.digitastuces.com",
    "bind_addr": "127.0.0.1",
    "client_addr": "127.0.0.1",
    "ports": {
      "http": -1,
      "https": 8501
    },
    "cert_file": "{{tls_cert_file}}",
    "key_file": "{{tls_key_file}}",
    "bootstrap_expect": 1,
    "ui": true,
    "data_dir": "/var/consul",
    "enable_debug": true,
    "log_level": "info",
    "addresses": {
      "http": "0.0.0.0",
      "https": "0.0.0.0"
    },
   "connect": {
    "enabled": true
   },
   "acl": {
    "enabled": true,
    "default_policy": "allow",
    "enable_token_persistence": true
   }
}